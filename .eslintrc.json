{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": ["error", "always",{ "omitLastInOneLineBlock": true}],
        "no-unreachable":["off"],
        "no-useless-escape": ["off"]
    }
}
